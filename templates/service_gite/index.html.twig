{% extends 'base.html.twig' %}

{% block title %}ServiceGite index{% endblock %}

{% block body %}
    <h1>ServiceGite index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Price</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for service_gite in service_gites %}
            <tr>
                <td>{{ service_gite.id }}</td>
                <td>{{ service_gite.price }}</td>
                <td>
                    <a href="{{ path('app_service_gite_show', {'id': service_gite.id}) }}">show</a>
                    <a href="{{ path('app_service_gite_edit', {'id': service_gite.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_service_gite_new') }}">Create new</a>
{% endblock %}
